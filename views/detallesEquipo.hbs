<div class="supercopa">
<h1>Detalles del Equipo - {{ equipo.nombre }}</h1>
    
    
    <div>
        <h2>Información del Equipo</h2>
        <p><strong>Nombre:</strong> {{ equipo.nombre }}</p>
        <p><strong>Conferencia:</strong> {{ equipo.conferencia }}</p>
        <p><strong>Historia:</strong> {{ equipo.historia }}</p>
        <p><strong>Títulos:</strong> {{ equipo.titulos }}</p>
        <p><strong>Sede:</strong> {{ equipo.sede }}</p>
        {{#each  leyendaArr}}
         <p><strong>Leyenda:</strong> {{ this.nombre }}</p>
         {{/each}}
    </div>
    
    
    <a href="/conferencias">Volver a la lista de conferencias</a>

</div>
